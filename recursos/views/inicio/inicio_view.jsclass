	render : function() {
		$('headerFertilizacionLogo').addEvent('click', function(){
			this.gotoNodos();
			this.drawTree('all');
		}.bind(this));
		$('headerBotonFoto').addEvent('click', function(){
			this.gotoNodos();
			this.drawTree('image');
		}.bind(this));
		$('headerBotonVideo').addEvent('click', function(){
			this.gotoNodos();
			this.drawTree('video');
		}.bind(this));
			
